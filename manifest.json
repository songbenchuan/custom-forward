{
  "manifest_version": 2,
  "name": "custom-forward",
  "description": "customize your forward",
  "version": "0.0.1",
  "permissions": [
    "storage"
  ],
  "content_scripts": [
    {
      "js": ["js/jquery-3.4.1.min.js", "js/content.js", "js/utils.js"],
      "matches": ["https://weibo.com/*"],
      "run_at": "document_end"
    }
  ]
}